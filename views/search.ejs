<div class="author">
    <h1 class="author__heading">Search Results</h1>
</div>

<section class="articles">

    <ul class="article-ul">
        <% data.forEach(post => { %>
        <li>
            <a href="/post/<%= post._id %>">
                <img src="/images/<%= post.imageURL %>" alt="Image" style="width: 15%; border-radius: 10px;">
                <span><%= post.title %></span>
                <span class="article-list__date"><%=
                    post.createdAt.toDateString() %></span>
            </a>
        </li>
        <% }) %>
    </ul>

</section>